{
	"name": "Fans Only Extension",
	"description": "This extension will only show to fans.",
	"version": "1.0",
	"manifest_version": 3,
	"content_scripts": [
		{
			"matches": ["https://onlyfans.com/*"],
			"js": ["contentScript.js"],
			"css": ["contentScript.css"]
		}
	],

	"background": {
		"service_worker": "background.js"
	},
	"permissions": ["tabs"],
	"options_page": "options.html",
	"action": {
		"default_popup": "popup.html"
	},

	"host_permissions": ["https://onlyfans.com/*"]
}
